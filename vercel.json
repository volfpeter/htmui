{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=172800, s-maxage=172800, stale-while-revalidate=86400"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/basecoat_app/main.py" }]
}
